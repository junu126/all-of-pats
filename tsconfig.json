{
  "compilerOptions": {
    "isolatedModules": true,
    "strict": true,
    "target": "ES2015",
    "allowJs": false,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true
  },
}
